name_jp = "東方花映塚　～ Phantasmagoria of Flower View"
name_en = "Touhou Kaeidzuka ~ Phantasmagoria of Flower View"
artist = "ZUN"
year = 2005
gamenum = "09"
lossless = true

# BGM Packing Method
# 0 = BGM Directory (only th06)
# 1 = single data file (usually thbgm.dat)
